<% if current_user && current_user.admin == true %>
	<span class="color"><h2>All Online Listings</h2></span>
	<%= link_to "[Search for Product Listing]", search_listings_path, class: "colorlink" %>
	<br><br>
	<% @listings.each do |listing| %>
		<%= link_to listing.product.name, category_product_path(listing.product.category.id, listing.product.id) %>
		<br>
		Website: <span class="color"><%= listing.website %></span>
		<br>
		<%= image_tag listing.product.image.url(:medium) %>
		<br><br>
	<% end %>
<% end %>